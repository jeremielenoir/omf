{
  "name": "node-js-getting-started",
  "version": "0.1.3",
  "description": "A sample Node.js app using Express 4",
  "main": "index.js",
  "scripts": {
    "start": "nodemon --inspect --exec babel-node -- ./app/src/bin/www",
    "start-prod": "NODE_ENV=production node app/dist/bin/www",
    "build-start-prod": "npm run build-back && npm run start-prod",
    "build-back": "rimraf app/dist/ && babel app/src --out-dir app/dist --ignore ./public/js --copy-files && ncp app/src/public/js app/dist/public/js",
    "watch": "grunt watch_server --force --silent",
    "start_no_server": "grunt watch_no_server --force --silent",
    "start_php": "grunt watch_php --force --silent",
    "build-front": "grunt compile --force"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.0.2",
    "babel-preset-env": "^1.6.1",
    "eslint": "^4.11.0",
    "grunt": "^1.0.1",
    "grunt-bower-concat": "^1.0.0",
    "grunt-contrib-clean": "^1.1.0",
    "grunt-contrib-connect": "^1.0.2",
    "grunt-contrib-handlebars": "^1.0.0",
    "grunt-contrib-jshint": "^1.1.0",
    "grunt-contrib-requirejs": "^1.0.0",
    "grunt-contrib-sass": "^1.0.0",
    "grunt-contrib-uglify": "^3.1.0",
    "grunt-contrib-watch": "^1.0.0",
    "grunt-php": "^1.5.1",
    "jshint-stylish": "^2.2.1",
    "ncp": "^2.0.0",
    "nodemon": "^1.12.1",
    "rimraf": "^2.6.2"
  },
  "dependencies": {
    "aws-sdk": "^2.1.36",
    "basic-auth": "^2.0.0",
    "body-parser": "~1.18.2",
    "bower": "^1.4.1",
    "connect-flash": "^0.1.1",
    "cookie-parser": "~1.4.3",
    "cookie-session": "~2.0.0-beta.3",
    "copy-cli": "^1.2.1",
    "cors": "~2.8.4",
    "cron": "^1.0.9",
    "express": "^4.16.2",
    "express-handlebars": "~3.0.0",
    "fbgraph": "^1.0.0",
    "fs": "0.0.2",
    "ftp": "^0.3.10",
    "ftps": "^1.1.0",
    "gm": "^1.18.1",
    "imagemagick": "^0.1.3",
    "method-override": "~2.3.2",
    "mime": "^2.0.3",
    "mongo-dump-stream": "^0.3.2",
    "mongoose": "^4.1.9",
    "nodalytics": "^1.1.0",
    "node-sass": "^4.7.2",
    "passport": "~0.4.0",
    "passport-facebook": "~2.1.1",
    "passport-twitter": "~1.0.3",
    "paypal-rest-sdk": "^1.8.1",
    "request": "~2.83.0",
    "twitter": "^1.2.5",
    "underscore": "^1.8.3",
    "underscore-express": "1.0.1"
  },
  "engines": {
    "node": "0.12.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/heroku/node-js-getting-started"
  },
  "keywords": [
    "node",
    "heroku",
    "express"
  ],
  "license": "MIT"
}
